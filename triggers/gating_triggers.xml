				<name>Gating</name>
				<script>local code = matches[2]

char = char or {}
weaving = weaving or {}
gatecodes = {}

if char.name then
  if io.exists(getMudletHomeDir() .. "/"..char.name.."_gatecodes.lua") then
    table.load(getMudletHomeDir() .. "/"..char.name.."_gatecodes.lua", gatecodes)
  end
  if weaving.gatename then
    gatecodes[weaving.gatename] = code
    table.save(getMudletHomeDir() .. "/"..char.name.."_gatecodes.lua", gatecodes)
    cecho(
      "\n&lt;green&gt;Code '" .. code .. "' successfully stored to location '" .. weaving.gatename .. "'."
    )
  else
    cecho("\n&lt;red&gt;Unknown error! No location set to store code to.")
  end
  send("release")
else
  cecho("\n&lt;red&gt;Error! Character name not known.")
end</script>
